<%= bootstrap_form_tag html: { id: "distribute_instruction_form" } do |f| %>
  <%= f.text_field :from_container, disabled: true, class: "from_container" %>
  <%= f.text_field :from_well, hide_label: true, class: "from_well" %>
  <%= f.select :to_container, [], {}, label: "Container", class: "to_container" %>
  <%= f.text_field :to_well, hide_label: true, class: "to_well" %>
  <%= f.text_field :volume, class: "volume" %>
  <%= f.submit %>
<% end %>